.parent {
display: grid;
grid-template-columns: repeat(20, 1fr);
grid-template-rows: repeat(20, 1fr);
grid-column-gap: 0px;
grid-row-gap: 0px;
}

.div1 { grid-area: 1 / 1 / 9 / 13; }
.div2 { grid-area: 1 / 13 / 9 / 16; }
.div3 { grid-area: 1 / 16 / 15 / 21; }
.div4 { grid-area: 9 / 4 / 15 / 16; }
.div5 { grid-area: 9 / 1 / 21 / 4; }
.div6 { grid-area: 15 / 4 / 21 / 12; }
